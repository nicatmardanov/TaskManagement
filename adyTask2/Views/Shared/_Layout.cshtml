<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="ADY" />

    <title>ADY | @ViewData["Title"]</title>


    <link rel="stylesheet" href="~/Content/assets/css/bootstrap_1.css">
    <link rel="stylesheet" href="/Content/assets/js/jquery-ui/css/no-theme/jquery-ui-1.10.3.custom.min.css">
    <link rel="stylesheet" href="/Content/assets/css/font-icons/entypo/css/entypo.css">
    <link href="~/Content/assets/css/font-icons/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Noto+Sans:400,700,400italic">
    <link rel="stylesheet" href="/Content/assets/css/neon-theme.css">
    <link rel="stylesheet" href="/Content/assets/css/neon-forms.css">



    @RenderSection("style", false)
    <link rel="stylesheet" href="/Content/assets/css/neon-core.css">
    <link rel="stylesheet" href="/Content/assets/css/custom.css">

    <script src="/Content/assets/js/jquery-1.11.0.min.js"></script>

    <!--[if lt IE 9]><script src="/Content/assets/js/ie8-responsive-file-warning.js"></script><![endif]-->
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->


</head>
<body class="page-body">

    @RenderSection("Modal", false)

    <div class="page-container">
        <!-- add class "sidebar-collapsed" to close sidebar by default, "chat-visible" to make chat appear always -->

        @await Component.InvokeAsync("SideBarMenu")

        <div class="main-content">

            @await Component.InvokeAsync("LayoutTopNotification")

            <hr />
            <div class="super_container">
                @RenderBody()
            </div>

        </div>


    </div>

    <!-- Bottom Scripts -->
    <script src="/Content/assets/js/gsap/main-gsap.js"></script>
    <script src="/Content/assets/js/jquery-1.11.0.min.js"></script>
    @*<script src="/Content/assets/js/bootstrap.js"></script>*@
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <script src="/Content/assets/js/joinable.js"></script>
    <script src="/Content/assets/js/resizeable.js"></script>
    <script src="/Content/assets/js/neon-api.js"></script>
    <script src="/Content/assets/js/neon-custom.js"></script>
    <script src="/Content/assets/js/neon-demo.js"></script>

    @RenderSection("script", false)



</body>
</html>