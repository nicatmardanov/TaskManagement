
@{
    ViewData["Title"] = "İclasın ləğv edilməsi";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using adyTask2.Models

@section Modal{
    @await Component.InvokeAsync("TrainLoadingModal")
}

@{
    Meeting meeting = Model;
}

<div id="meeting_cancel" data-id="@ViewBag.MId">
    @await Component.InvokeAsync("MeetingShow", meeting)
    @{ await Html.RenderPartialAsync("CancelForm");}
</div>

@section script{
    <script src="~/Content/assets/js/meeting_cancel.js"></script>
}